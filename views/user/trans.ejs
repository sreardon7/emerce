<div class = 'container'>
	<div class = 'jumbotron'>
		<h1> Request Transaction History </h1>
 
		<div class = 'row'>

				<div class = 'col-md-6'>
					<div class = 'panel panel-default'>
						<div class = 'panel-heading'>
							<h3 class = 'panel-title'> Transaction </h3>
						</div>
						<div class = 'panel-body'>
							<form action = '/user/transactions' method = 'GET' class = 'form-horizontal' id = 'theform'>
								<div class = 'form-group'>
									<label for = 'from' class = 'col-sm-2'>From:</label>
									<div class = 'col-sm-10'>
										<input type = 'text' class = 'form-control form-light' placeholder = 'yyyy-mm-dd' name = 'from' id = 'from'>
									</div>
								</div>
								<div class = 'form-group'>
									<label class = 'col-sm-2' for = 'to'>To:</label>
									<div class = 'col-sm-10'>
										<input type = 'text' class = 'form-control form-light' placeholder = 'yyyy-mm-dd' name = 'to' id = 'to'>
									</div>
								</div>
								<div class = 'form-group'>
									<div class = 'col-sm-6'>
										<a href = '/user/transactions' class = 'btn btn-inverse btn-block'>Full History</a>
									</div>
									<div class = 'col-sm-6'>
										<input type = 'button' class = 'btn btn-inverse btn-block' onclick='suubmit()' value = 'Partial History'/>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>

				<div class = 'col-md-6'>
					<div class = 'panel panel-default'>
						<div class = 'panel-heading'>
							<h3 class = 'panel-title'> Included Fields </h3>
						</div>
						<div class = 'panel-body'>
							<table class = 'table'>
								<tr>
									<td>
										<span class = 'glyphicon glyphicon-stats'> </span>
										Transaction ID</td>
									<td>Date</td>
									<td>Account #</td>
									<td>Description</td>
								</tr>
								<tr>
									<td>Commission</td>
									<td>Type</td>
									<td>Start Balance</td>
									<td>End Balance</td>
								</tr>
								<tr>
									<td>Amount</td>
									<td>Store ID</td>
									<td>Country</td>
									<td>Carrier</td>
								</tr>
								<tr>
									<td>Mobile Phone</td>
									<td>Product Type</td>
									<td>Can Undo</td>
									<td>Country Code</td>
								</tr>
								<tr>
									<td>Transaction Code</td>
									<td></td>
									<td></td>
									<td></td>
								</tr>
							</table>
						</div>
					</div>
				</div>
		</div>
	</div>
	<h1 id = 'lar'> blue </h1>
	<h1 id = 'err'> err </h1>
	<h1 id = 'far'> to </h1>
</div> 



<script> 
	function suubmit() {
		document.getElementById('err').innerHTML = 'works';
		var date1 = document.getElementById('from').value; 
		var date2 = document.getElementById('to').value;
		document.getElementById('lar').innerHTML = date1;
		document.getElementById('far').innerHTML = date2;

	//	document.getElementById('theform').submit();
	}
</script>
						


